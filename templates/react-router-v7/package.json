{
  "name": "react-router-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "react-router dev",
    "build": "react-router build",
    "start": "react-router-serve ./build/server/index.js",
    "typegen": "react-router typegen && tsc",
    "lint": "eslint",
    "precommit": "lint-staged",
    "format": "prettier --write **/*.{ts,tsx,css} --config ./prettier.config.js",
    "test:vt": "cross-env NODE_ENV=test vitest",
    "test:vt:exec": "cross-env NODE_ENV=test vitest run",
    "test:vt:coverage": "cross-env NODE_ENV=test vitest --coverage.enabled",
    "test:vt:ui": "cross-env NODE_ENV=test vitest --ui --open false",
    "test:vt:report": "http-server coverage -p 51730",
    "test:pw:headless": "playwright test",
    "test:pw:headed": "playwright test --headed",
    "test:pw:ui": "playwright test --ui",
    "test:pw:codegen": "playwright codegen",
    "test:pw:report": "playwright show-report",
    "sb:dev": "cross-env STORYBOOK=true storybook dev -p 6006 -c ./src/config/storybook --quiet --no-open",
    "sb:build": "cross-env STORYBOOK=true storybook build -c ./src/config/storybook",
    "sb:preview": "http-server storybook-static -p 6600",
    "db:push": "drizzle-kit push",
    "db:migrate": "drizzle-kit migrate",
    "db:generate": "drizzle-kit generate",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*.{ts,tsx,css}": "npm run format"
  },
  "dependencies": {
    "@react-router/node": "^7.7.1",
    "@react-router/serve": "^7.7.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.6",
    "envalid": "^8.1.0",
    "isbot": "^5.1.27",
    "mysql2": "^3.15.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.7.1",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@playwright/test": "^1.56.1",
    "@storybook/addon-a11y": "^9.1.4",
    "@storybook/addon-docs": "^9.1.8",
    "@react-router/dev": "^7.7.1",
    "@storybook/react-vite": "^9.1.13",
    "@tailwindcss/postcss": "^4.1.14",
    "@tailwindcss/vite": "^4.1.4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.1.0",
    "drizzle-kit": "^0.31.5",
    "eslint": "^9.38.0",
    "eslint-plugin-storybook": "^9.1.13",
    "http-server": "^14.1.1",
    "husky": "^9.1.7",
    "jsdom": "^27.0.1",
    "lint-staged": "^16.2.4",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "storybook": "^9.1.13",
    "storybook-addon-remix-react-router": "^5.0.0",
    "tailwindcss": "^4.1.4",
    "tsx": "^4.20.6",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.46.1",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
